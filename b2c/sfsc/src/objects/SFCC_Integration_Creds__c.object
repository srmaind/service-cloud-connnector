<?xml version="1.0" encoding="UTF-8"?>
<CustomObject xmlns="http://soap.sforce.com/2006/04/metadata">
    <customSettingsType>Hierarchy</customSettingsType>
    <description>This custom setting keeps SFCC Client Credentials.</description>
    <enableFeeds>false</enableFeeds>
    <fields>
        <fullName>Client_Id__c</fullName>
        <caseSensitive>false</caseSensitive>
        <defaultValue>&apos;aaaaaaaaaaaaaaaaaaaaaaaaaaaaaa&apos;</defaultValue>
        <description>Client id configured in SFCC for SF. Default value for sandbox is aaaaaaaaaaaaaaaaaaaaaaaaaaaaaa</description>
        <externalId>false</externalId>
        <inlineHelpText>Client id configured in SFCC for SF. Default value for sandbox is aaaaaaaaaaaaaaaaaaaaaaaaaaaaaa</inlineHelpText>
        <label>Client Id</label>
        <length>255</length>
        <required>false</required>
        <trackTrending>false</trackTrending>
        <type>Text</type>
        <unique>true</unique>
    </fields>
    <fields>
        <fullName>Client_Secret__c</fullName>
        <caseSensitive>false</caseSensitive>
        <defaultValue>&apos;aaaaaaaaaaaaaaaaaaaaaaaaaaaaaa&apos;</defaultValue>
        <description>This field holds Client Secret configured in SFCC account manager for SF. Default value for sandbox is aaaaaaaaaaaaaaaaaaaaaaaaaaaaaa</description>
        <externalId>false</externalId>
        <inlineHelpText>This field holds Client Secret configured in SFCC account manager for SF. Default value for sandbox is aaaaaaaaaaaaaaaaaaaaaaaaaaaaaa</inlineHelpText>
        <label>Client Secret</label>
        <length>255</length>
        <required>false</required>
        <trackTrending>false</trackTrending>
        <type>Text</type>
        <unique>true</unique>
    </fields>
    <label>SFCC Integration Creds</label>
    <visibility>Public</visibility>
</CustomObject>
